---
import BaseLayout from '../layouts/BaseLayout.astro';
import InteractiveLanguageList from '../components/InteractiveCardSection';
import type { Language } from '../utils/language';
import languagesData from '../data/languages.json';

const totalLanguages = languagesData.length;
---

<BaseLayout count={totalLanguages}>
  <InteractiveLanguageList languages={languagesData as Language[]} client:load />
</BaseLayout>